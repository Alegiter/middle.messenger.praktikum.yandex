{
  "name": "y.praktikum.middle-frontend",
  "version": "0.1.0",
  "description": "Solo chat application",
  "browserslist": [
    "defaults"
  ],
  "scripts": {
    "start": "parcel serve --no-cache  -p 3000 src/static/*.html --dist-dir dist",
    "prestart": "npm run prebuild",
    "build": "parcel build --no-cache --no-source-maps src/static/*.html --dist-dir dist",
    "prebuild": "npm run tsc && npm run clear",
    "clear": "rm -rf ./dist",
    "tsc": "tsc",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "handlebars": "^4.7.7"
  },
  "devDependencies": {
    "@parcel/validator-typescript": "^2.0.0-alpha.3",
    "parcel": "^2.0.0-beta.3.1",
    "postcss-import": "^14.0.2",
    "precss": "^4.0.0",
    "typescript": "^4.3.5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Alegiter/y.praktikum.middle-frontend.git"
  },
  "author": "Vladimir Sitnik",
